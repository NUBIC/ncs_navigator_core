- page_title "Field Activity Report"

.page_section
  %table
    %tr
      %th ID
      %td.fieldwork_id
        = @fieldwork.fieldwork_id
    %tr
      %th Generated by
      %td.staff_id
        = @fieldwork.staff_id
    %tr
      %th Client ID
      %td.client_id
        = @fieldwork.client_id

.page_section
  - if @conflicts
    = render 'conflict_report', :report => @conflicts
